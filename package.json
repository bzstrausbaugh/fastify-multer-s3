{
  "name": "fastify-multer-s3",
  "version": "0.1.0",
  "description": "Fastify streaming multer storage engine for AWS S3",
  "main": "./lib/cjs/index.cjs",
  "types": "./lib/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "require": {
        "types": "./lib/index.d.cts",
        "default": "./lib/cjs/index.cjs"
      },
      "import": {
        "types": "./lib/index.d.ts",
        "default": "./lib/esm/index.js"
      }
    }
  },
  "files": [
    "lib/esm/index.js",
    "lib/cjs/index.cjs",
    "lib/index.d.ts",
    "lib/index.d.cts"
  ],
  "scripts": {
    "test": "mocha tests/**/*.js",
    "clean": "rm -rf ./lib",
    "build:types": "npx tsup ./index.ts --out-dir lib --dts-only --format esm,cjs",
    "build:cjs": "tsc -p tsconfig.cjs.json && mv ./lib/cjs/index.js ./lib/cjs/index.cjs",
    "build:esm": "tsc -p tsconfig.json",
    "build:code": "npm run build:cjs && npm run build:esm",
    "build": "npm run build:types && npm run build:code",
    "build:clean": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bzstrausbaugh/fastify-multer-s3"
  },
  "keywords": [
    "fastify",
    "multer",
    "s3",
    "amazon",
    "aws"
  ],
  "authors": [
    "badunk",
    "nociza",
    "bzstrausbaugh"
  ],
  "license": "MIT",
  "homepage": "https://github.com/bzstrausbaugh/fastify-multer-s3#readme",
  "dependencies": {
    "magic-bytes.js": "^1.10.0",
    "run-parallel": "^1.2.0",
    "is-svg": "5.0.0"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.12.4",
    "@types/run-parallel": "^1.1.2",
    "@swc/core": "^1.4.13",
    "@aws-sdk/client-s3": "^3.550.0",
    "mock-req": "^0.2.0",
    "ts-node": "^10.9.2",
    "concat-stream": "^1.5.1",
    "fastify": "^4.26.2",
    "fastify-plugin": "^4.5.1",
    "fastify-multer": "^2.0.3",
    "form-data": "^1.0.0-rc3",
    "mocha": "^10.2.0",
    "on-finished": "^2.3.0",
    "xtend": "^4.0.1",
    "tsup": "^8.0.2"
  },
  "typings": "./index.d.ts"
}
